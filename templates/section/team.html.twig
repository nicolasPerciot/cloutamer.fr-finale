<!-- Team-->
<section class="page-section bg-first text-dark" id="team">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading text-uppercase">{{ features["team"]["title"] }}</h2>
            {# <h3 class="section-subheading text-muted">Lorem ipsum dolor sit amet consectetur.</h3> #}
        </div>
        <div class="row">
            <div class="col">

                {% for team in features["team"]["content"] %}
                    <div class="team-member">
                        <img class="mx-auto rounded-circle" src={{ asset(team["img"]) }} alt="..." />
                        <h4>{{ team["name"] }}</h4>
                        <p class="text-muted">{{ team["role"] }}</p>
                        <a class="btn btn-dark btn-social mx-2" href="{{ team["linkedin"] }}" target="_blank" rel="noopener noreferrer"><i class="fab fa-linkedin-in"></i></a>
                        <a class="btn btn-dark btn-social mx-2" href="{{ team["github"] }}" target="_blank" rel="noopener noreferrer"><i class="fab fa-github"></i></a>
                    </div>

                {% endfor %}
            </div>            
        </div>

        
        {# <div class="row">
            <div class="col-lg-8 mx-auto text-center"><p class="large text-muted">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Aut eaque, laboriosam veritatis, quos non quis ad perspiciatis, totam corporis ea, alias ut unde.</p></div>
        </div> #}
    </div>
</section>